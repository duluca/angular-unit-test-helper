{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/tests",
    "**/coverage",
    "*.conf*.js"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "branches": 50,
  "lines": 75,
  "functions": 50,
  "statements": 50
}